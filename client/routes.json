[
  {
    "path": "/",
    "view": "./views/stationsearch",
    "chunk": "main"
  },
  {
    "path": "/arrivals/:stopPointId",
    "view": "./views/arrivals",
    "chunk": "main",
    "data": {
      "arrivals": "GET https://api.tfl.gov.uk/StopPoint/$stopPointId/Arrivals"
    }
  },
  {
    "path": "/error",
    "view": "./views/error",
    "chunk": "main"
  },
  {
    "path": "/about",
    "view": "./views/about"
  },
  {
    "path": "/tasks/:status(pending|completed)?",
    "view": "./views/home"
  }
]
